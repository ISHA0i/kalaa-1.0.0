# API Configuration
REACT_APP_API_URL=http://localhost:5002/api
REACT_APP_BASE_URL=http://localhost:3001

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_LOGGING=true

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# App Configuration
REACT_APP_NAME=Kalaa Art Gallery
REACT_APP_DESCRIPTION=Your one-stop shop for unique art pieces
REACT_APP_VERSION=1.0.0

# Image Configuration
REACT_APP_MAX_IMAGE_SIZE=5242880  # 5MB in bytes
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# Cache Configuration
REACT_APP_CACHE_DURATION=3600  # 1 hour in seconds 